list()
	.list
		.content
			.navigator
				overflow hidden
				margin-top 10px
			.detail
				overflow hidden		
				
		.navigator .sub
			width 24.7%
			height 30px
			line-height 30px
			border 1px solid #5D9DDA
			border-right none
			text-align center
			display block
			float left
			.last
				border-right 1px solid #5D9DDA
		.detail
			.menu
				width 10%
				float left
			.main
				width 90%
				float left
				
		.menu .sub
			width 100%
			height 30px
			line-height 30px
			text-align center
			color #fff
			border-bottom 1px solid #fff
			background #539dcc
			a
				color #fff
		.main .item 
			float left
			width 100%
			margin-bottom 3px
				
		.main .item
			.pic
				width 50%
				height 80px
				float left
				background #00ffdd
				text-align center
			.note
				width 50%
				float left

		.note 
			.name
				color #333
				height 30px
				line-height 30px
				text-align center
				font-size 14px
				border 1px solid #5D9DDA
				border-bottom none
			.service
				color #999
				height 25px
				line-height 25px
				text-align center
				font-size 12px
				border 1px solid #5D9DDA
				border-bottom none
			.extral
				height 20px
				line-height 20px
				color #999
				overflow hidden
				font-size 12px
				border 1px solid #5D9DDA
		.extral
			span
				font-size 14px
				color #fff
				float left
			em
				color #b4806b
				font-size 14px
				margin-left 10px
				font-weight normal
			strong	
				float right 
				color #999
				margin-right  10px
				font-weight normal